{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KeyEntryCache": {
    "SlidingRetentionPeriod": 30
  },
  "LogCompaction": {
    "CompactionInterval": 60 
  },
  "WriteAheadLog": {
    "Location": "bin/wal"
  },
  "DataStore": {
    "Location": "bin/store"
  }
}
